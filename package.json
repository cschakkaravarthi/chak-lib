{
  "name": "umgc_ui_library",
  "version": "1.6.1",
  "description": "A visual library for our internal projects.",
  "main": "lib/index.js",
  "publishConfig": {
    "registry": "https://artifactory.umusic.net/artifactory/api/npm/umg-npm/"
  },
  "files": [
    "lib/**/*",
    "scss/**/*"
  ],
  "scripts": {
    "start": "start-storybook",
    "build": "npm run test && tsc && build-storybook && cp src/assets/images/*.svg src/assets/images/*.png src/assets/images/*.jpg lib/assets/images/",
    "test": "npm run lint && jest --coverage",
    "test:fix": "npm run lint:fix && jest --coverage -u",
    "lint": "eslint . --ext .ts,.tsx && stylelint scss/*.scss",
    "lint:fix": "eslint . --fix --ext .ts,.tsx && stylelint scss/*.scss --fix",
    "precommit": "npm run lint",
    "versionfile": "git log --pretty=format:'%h' -n 1 > version.txt"
  },
  "dependencies": {
    "@storybook/addon-info": "^5.3.7",
    "@storybook/react": "^5.3.7",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "file-loader": "^4.3.0",
    "react": "^16.12.0",
    "react-autosuggest": "^9.4.3",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-loading-skeleton": "^1.3.0",
    "react-transition-group": "^4.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^4.3.3",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "@types/react": "^16.9.15",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-test-renderer": "^16.9.1",
    "@types/react-transition-group": "^4.2.3",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "^2.17.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.1",
    "eslint": "6.3.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^11.1.1",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-scss": "^3.11.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
